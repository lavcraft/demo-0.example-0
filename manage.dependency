#!/bin/bash

ARRAY=( "dep0:last"
        "cmake:f8d21e88234d8026a339fd0f83917efc492d6799" )

for dependency in "${ARRAY[@]}" ; do
    KEY=${dependency%%:*}
    VALUE=${dependency#*:}
    printf "dependency %s version %s.\n" "$KEY" "$VALUE"
    pushd $KEY
    echo "KEY :$KEY"
    if [ "last" == "$KEY" ];then
	git pull origin master
    else
	git checkout $VALUE
    fi
    popd
done
